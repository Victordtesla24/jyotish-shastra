  },
  "performance_benchmarks": {
    "chart_generation_time": {
      "target": "< 2 seconds",
      "description": "Time to generate complete birth chart"
    },
    "analysis_completion_time": {
      "target": "< 5 seconds",
      "description": "Time to complete full astrological analysis"
    },
    "concurrent_requests": {
      "target": "50 simultaneous users",
      "description": "System should handle multiple concurrent chart generations"
    }
  },
  "data_quality_checks": {
    "coordinate_precision": {
      "decimal_places": 4,
      "description": "Latitude/longitude should have at least 4 decimal places"
    },
    "time_precision": {
      "format": "HH:MM:SS",
      "description": "Time should include seconds for accuracy"
    },
    "timezone_validation": {
      "format": "IANA timezone identifier",
      "examples": ["Asia/Kolkata", "America/New_York", "Europe/London"]
    }
  }
