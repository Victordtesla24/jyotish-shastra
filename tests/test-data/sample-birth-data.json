  "validationRules": {
    "dateRange": {
      "minimum": "1900-01-01",
      "maximum": "2100-12-31"
    },
    "timeFormat": "HH:MM:SS",
    "coordinateRanges": {
      "latitude": {
        "minimum": -90,
        "maximum": 90
      },
      "longitude": {
        "minimum": -180,
        "maximum": 180
      }
    },
    "requiredFields": [
      "dateOfBirth",
      "timeOfBirth",
      "placeOfBirth.name",
      "placeOfBirth.latitude",
      "placeOfBirth.longitude",
      "placeOfBirth.timezone"
    ]
  },
  "testScenarios": {
    "basic_chart_generation": {
      "description": "Test basic chart generation functionality",
      "testCases": ["test_case_1", "test_case_2", "test_case_3"],
      "expectedOutputs": [
        "rasiChart",
        "navamsaChart",
        "planetaryPositions",
        "housePositions",
        "ascendantDegree"
      ]
    },
    "yoga_detection": {
