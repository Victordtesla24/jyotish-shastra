      "testCases": ["test_case_1"],
      "expectedOutputs": [
        "gajaKesariYoga",
        "rajaYogas",
        "dhanaYogas",
        "arishtaYogas"
      ]
    },
    "strength_calculation": {
      "description": "Test planetary strength calculations",
